  "scripts": {
    "lint": "eslint --ext .tsx,.ts src/",
    "typecheck": "tsc --noemit",
    "build": "gatsby build",
    "build:ci": "CYPRESS_SUPPORT=y GATSBY_CPU_COUNT=2 gatsby build",
    "develop": "CYPRESS_SUPPORT=y gatsby develop -H 0.0.0.0 -p 8001",
    "develop:preact": "CYPRESS_SUPPORT=y gatsby develop -H 0.0.0.0 -p 8001",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "start:prod": "NODE_ENV=production npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "storybook": "cp -r ./public/page-data/sq/d ./public/static && start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s .storybook/static/",
    "gen:component": "hygen component new --name",
    "test": "cp -r ./public/page-data/sq/d ./public/static && TZ=UTC jest --coverage --passWithNoTests --coverageReporters json clover",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
