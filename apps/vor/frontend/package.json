    "develop": "cross-env CYPRESS_SUPPORT=y gatsby develop -H 0.0.0.0 -p 8001",
    "develop:preact": "cross-env CYPRESS_SUPPORT=y gatsby develop -H 0.0.0.0 -p 8001",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "start:prod": "NODE_ENV=production npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "storybook": "cross-env NODE_ENV=production start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s .storybook/static/",
    "gen:component": "hygen component new --name",
